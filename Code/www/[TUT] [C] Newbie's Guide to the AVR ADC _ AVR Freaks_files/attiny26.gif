function ui_reseller() 
{
    if(!e('use_reseller_price_input').checked)
    {
        e('reseller_price_input').readonly = true;
        e('reseller_price_input').disabled = true;
        e('reseller_price_input').placeholder = '';
        //e('reseller_price_input').value = '';
    }
    else
    {
        e('reseller_price_input').readonly = false;
        e('reseller_price_input').disabled = false;
        e('reseller_price_input').placeholder = e('buynow_price_input').value ? e('buynow_price_input').value : 'none';
    }
    
    
    if ($('#sp_rc').length > 0 && $('#sp_rentalrule').length > 0)
    {
        if(e('buynow_price_input').value && e('buynow_price_input').value != 0)
        {
            e('sp_rc').disabled = false;
            e('sp_rentalrule').disabled = false; 
        }
        else
        {
            e('sp_rc').checked = false;
            e('sp_rc').disabled = true;
            e('sp_rp').value = 0;
            e('sp_rp').disabled = true;
            $('#sp_rpholder').hide();
            $('#sp_rentalrule').val(0);
            e('sp_rentalrule').disabled = true;
        }
    }
    
}

function ui_setmarket()
{
    var name = 'wkretak.pl';
    var p1 = e('min_price_input').value ? e('min_price_input').value : 0;
    var p2 = e('buynow_price_input').value ? e('buynow_price_input').value : 0;
    var p3 = e('reseller_price_input').value ? e('reseller_price_input').value : 0;
    var curr = e('currency_input').options[e('currency_input').selectedIndex].value;
    
    var rs = $('#sp_rentalrule').val();
    var rp = $('#sp_rp').val();
    if(rs == '')
        rs = 0;
    if(!e('sp_rc').checked || e('sp_rc').disabled)
        rp = 0;
    
    
    if(!e('use_reseller_price_input').checked) p3 = -1;
    ajax_image('change_price_button', 0, '');
    
    AM_setDomainPrices(name, true, p2, p1, curr, p3, rs, rp, function(r) { 
        if(ajax_check2('change_price_button', r)) {
            if(parseInt(p2) > 0) {
                $('#buynow_box').show();
                $('#price_info_title_holder').html('Buy It Now price:');
                $('#price_info_holder').html('<big id="buynow_price">'+parseInt(p2)+'</big> <big id="buynow_currency" class="buynow_currency">'+curr+'</big>');
                if(parseInt(rs) > 0)
                {
                    var rs_min_price = Math.ceil(min_i_price[parseInt(rs)] * parseInt(p2));
                    $('#installment_box').show();
                    $('#installment_box .lowdesc2').html(rs_min_price).append(' ' + curr).append(' <span style="font-size: 13pt">/ mth.</span>');
                    $('#installment_box .lowdesc2').css("font-size", "22pt");
                    if(rs_min_price > 9999 && rs_min_price < 100000)
                        $('#installment_box .lowdesc2').css("font-size", "20pt");
                    if(rs_min_price > 99999)
                        $('#installment_box .lowdesc2').css("font-size", "19pt");
                }
                else
                {
                    $('#installment_box').hide();
                }
                
                if(parseInt(rp) > 0)
                {
                    $('#rental_box').show();
                    $('#rental_box .lowdesc2').html(Math.ceil(parseFloat(rp)*parseFloat(p2)/100)).append(' ' + curr).append(' <span style="font-size: 13pt">/ mth.</span>');
                }
                else
                {
                    $('#rental_box').hide();
                }
            } else {
                $('#installment_box').hide();
                $('#rental_box').hide();
                $('#buynow_box').hide();
                $('#price_info_title_holder').html('');
                $('#price_info_holder').html('Price for negotiation');
            }
            $('#buynow_box .lowdesc2').html(parseInt(p2));
            $('#buynow_box .lowdesc2').append(' '+curr);
            $('#offer_box .lowdesc2 span').html(curr);
            
            if(p2 == 0 || parseInt(p2) > parseInt(p1))
                $('#offer_box').show();
            else
                $('#offer_box').hide();
                

            $.ajax({
                url: "resync_ajax.php?name=" + name,
                cache: false,
                success: function () {}
            }); 
        } 
    });
}



